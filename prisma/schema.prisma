generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Blog {
  id         String   @id @default(uuid())
  title      String
  coverImage String
  content    String
  isFeatured Boolean  @default(false)
  createdAt  DateTime @default(now())
}
